{
  "name": "Sky News",
  "version": "1.0",
  "description": "Sky News - allows you to see the latest news and once a breaking story comes you'll see a notification",
  "browser_action": {
    "default_icon": "news_skynews_icon-16.png",
	 "popup": "buttonPopup.html"
  },
  "icons": {
    "128": "news_skynews_icon-128.png",
     "48": "news_skynews_icon-48.png",
     "32": "news_skynews_icon-32.png",
     "16":"news_skynews_icon-16.png"
  },
  "background_page": "background.html",
  "options_page": "options.html",
  "permissions": [
	"tabs",
	"notifications",
	"contextMenus",
	"http://skynewstwitterproxy.appspot.com/*",
	"http://localhost/*",
	"http://news.sky.com/*",
	"http://api.twitter.com/*",
	"http://blogs.news.sky.com/rss/*"
  ]
}
